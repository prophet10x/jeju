{
  "name": "@jejunetwork/shared",
  "version": "0.1.0",
  "description": "Shared hooks, components, APIs, services, protocols, auth, and cryptography",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./protocols/middleware": {
      "bun": "./src/protocols/middleware.ts",
      "types": "./dist/protocols/middleware.d.ts",
      "import": "./dist/protocols/middleware.js"
    },
    "./wagmi": {
      "bun": "./src/wagmi/index.ts",
      "types": "./dist/wagmi/index.d.ts",
      "import": "./dist/wagmi/index.js"
    }
  },
  "files": ["dist", "src"],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.3.0",
    "@jejunetwork/config": "workspace:*",
    "@jejunetwork/contracts": "workspace:*",
    "@jejunetwork/db": "workspace:*",
    "@jejunetwork/types": "workspace:*",
    "@noble/hashes": "1.7.1",
    "clsx": "^2.1.1",
    "croner": "^9.0.0",
    "elysia": "^1.4.19",
    "nanoid": "^5.1.5",
    "pino": "^10.1.0",
    "pretty-bytes": "^6.1.1",
    "pretty-ms": "^9.2.0",
    "siwe": "^2.3.2",
    "tailwind-merge": "^2.0.0",
    "timeago.js": "^4.0.2",
    "viem": "2.43.3",
    "zod": "4.2.1"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "22.15.30",
    "@types/react": "19.2.3",
    "pino-pretty": "^13.0.0",
    "typescript": "5.9.3"
  },
  "optionalDependencies": {
    "pino-pretty": "^13.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elizaos/jeju.git",
    "directory": "packages/shared"
  },
  "publishConfig": {
    "access": "public"
  }
}
