{
  "name": "@jejunetwork/shared",
  "version": "0.1.0",
  "description": "Shared hooks, components, APIs, services, protocols, database layer, auth, and cryptography for Jeju apps",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./auth": {
      "types": "./dist/auth/index.d.ts",
      "import": "./dist/auth/index.js"
    },
    "./auth/siwe": {
      "types": "./dist/auth/siwe.d.ts",
      "import": "./dist/auth/siwe.js"
    },
    "./auth/passkeys": {
      "types": "./dist/auth/passkeys.d.ts",
      "import": "./dist/auth/passkeys.js"
    },
    "./auth/siwf": {
      "types": "./dist/auth/siwf.d.ts",
      "import": "./dist/auth/siwf.js"
    },
    "./auth/types": {
      "types": "./dist/auth/types.d.ts",
      "import": "./dist/auth/types.js"
    },
    "./auth/hooks": {
      "types": "./dist/auth/hooks.d.ts",
      "import": "./dist/auth/hooks.js"
    },
    "./auth/provider": {
      "types": "./dist/auth/provider.d.ts",
      "import": "./dist/auth/provider.js"
    },
    "./chains": {
      "types": "./dist/chains/index.d.ts",
      "import": "./dist/chains/index.js"
    },
    "./services": {
      "types": "./dist/services/index.d.ts",
      "import": "./dist/services/index.js"
    },
    "./protocols": {
      "types": "./dist/protocols/index.d.ts",
      "import": "./dist/protocols/index.js"
    },
    "./crypto": {
      "types": "./dist/crypto/index.d.ts",
      "import": "./dist/crypto/index.js"
    },
    "./db": {
      "types": "./dist/db/index.d.ts",
      "import": "./dist/db/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elizaos/jeju.git",
    "directory": "packages/shared"
  },
  "keywords": [
    "jeju",
    "shared",
    "auth",
    "siwe",
    "protocols",
    "services"
  ],
  "dependencies": {
    "@jejunetwork/db": "workspace:*",
    "@jejunetwork/kms": "workspace:*",
    "@jejunetwork/config": "workspace:*",
    "@jejunetwork/types": "workspace:*",
    "elysia": "^1.3.0",
    "@elysiajs/cors": "^1.3.0",
    "viem": "2.43.2",
    "wagmi": "2.18.1",
    "zod": "^3.24.1",
    "opossum": "^9.0.0",
    "p-retry": "^7.1.1",
    "pino": "^10.1.0",
    "siwe": "^2.3.2",
    "croner": "^9.0.0",
    "ioredis": "^5.4.1",
    "pretty-bytes": "^6.1.1",
    "pretty-ms": "^9.2.0",
    "timeago.js": "^4.0.2",
    "nanoid": "^5.1.5",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "22.15.30",
    "@types/react": "19.2.3",
    "typescript": "5.9.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
