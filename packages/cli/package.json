{
  "name": "@jejunetwork/cli",
  "version": "0.1.0",
  "description": "Network CLI - Development, testing, deployment, and key management",
  "license": "MIT",
  "type": "module",
  "bin": {
    "jeju": "./bin/jeju.js",
    "git-remote-jeju": "./bin/git-remote-jeju.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./keys": {
      "import": "./dist/lib/keys.js"
    },
    "./chain": {
      "import": "./dist/lib/chain.js"
    },
    "./testing": {
      "import": "./dist/lib/testing.js"
    },
    "./types": {
      "import": "./dist/types.js"
    },
    "./git-remote": {
      "import": "./dist/commands/git-remote.js"
    }
  },
  "files": [
    "dist",
    "bin",
    "templates"
  ],
  "scripts": {
    "build": "bun build src/index.ts src/commands/git-remote.ts --outdir dist --target bun --external electron --external playwright-core",
    "dev": "bun run src/index.ts",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'Lint passed'",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@jejunetwork/config": "workspace:*",
    "@jejunetwork/shared": "workspace:*",
    "@jejunetwork/types": "workspace:*",
    "chalk": "5.4.1",
    "commander": "12.1.0",
    "execa": "9.5.3",
    "elysia": "1.3.0",
    "@elysiajs/cors": "1.3.0",
    "prompts": "2.4.2",
    "viem": "2.43.2",
    "which": "4.0.0",
    "@noble/curves": "2.0.1",
    "@noble/hashes": "1.7.1"
  },
  "optionalDependencies": {
    "@phala/dstack-sdk": "0.5.7"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/prompts": "2.4.9",
    "@types/which": "3.0.4",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "@jejunetwork/config": ">=0.1.0",
    "@jejunetwork/contracts": ">=0.2.0",
    "@jejunetwork/types": ">=0.1.0",
    "@jejunetwork/tests": ">=0.1.0",
    "@playwright/test": ">=1.55.0",
    "@synthetixio/synpress": ">=4.0.0",
    "zod": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "@jejunetwork/config": { "optional": true },
    "@jejunetwork/contracts": { "optional": true },
    "@jejunetwork/types": { "optional": true },
    "@jejunetwork/tests": { "optional": true },
    "@playwright/test": { "optional": true },
    "@synthetixio/synpress": { "optional": true },
    "zod": { "optional": true }
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "os": ["darwin", "linux"],
  "cpu": ["x64", "arm64"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elizaos/jeju.git",
    "directory": "packages/cli"
  },
  "homepage": "https://jejunetwork.org",
  "bugs": {
    "url": "https://github.com/elizaos/jeju/issues"
  },
  "keywords": [
    "jeju",
    "cli",
    "blockchain",
    "ethereum",
    "l2",
    "optimism",
    "development",
    "testing",
    "deployment",
    "foundry",
    "forge"
  ],
  "publishConfig": {
    "access": "public"
  }
}
