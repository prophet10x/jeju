{
  "name": "@jejunetwork/cli",
  "version": "0.1.0",
  "description": "Network CLI - Development, testing, deployment, and key management",
  "license": "MIT",
  "type": "module",
  "bin": {
    "jeju": "./bin/jeju.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./keys": {
      "import": "./dist/lib/keys.js"
    },
    "./chain": {
      "import": "./dist/lib/chain.js"
    },
    "./testing": {
      "import": "./dist/lib/testing.js"
    },
    "./types": {
      "import": "./dist/types.js"
    }
  },
  "files": [
    "dist",
    "bin",
    "templates"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target bun",
    "dev": "bun run src/index.ts",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'Lint passed'",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "ethers": "^6.11.1",
    "execa": "^9.0.0",
    "fs-extra": "^11.2.0",
    "hono": "^4.6.10",
    "ora": "^8.0.1",
    "prompts": "^2.4.2",
    "viem": "^2.7.15",
    "which": "^4.0.0",
    "zod": "^3.22.4",
    "@noble/curves": "^1.3.0",
    "@noble/hashes": "^1.3.3"
  },
  "optionalDependencies": {
    "@phala/dstack-sdk": "^0.5.7"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/fs-extra": "^11.0.4",
    "@types/prompts": "^2.4.9",
    "@types/which": "^3.0.3",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@jejunetwork/config": "^0.1.0",
    "@jejunetwork/contracts": "^0.2.0",
    "@jejunetwork/types": "^0.1.0"
  },
  "peerDependenciesMeta": {
    "@jejunetwork/config": { "optional": true },
    "@jejunetwork/contracts": { "optional": true },
    "@jejunetwork/types": { "optional": true }
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "os": ["darwin", "linux"],
  "cpu": ["x64", "arm64"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elizaos/jeju.git",
    "directory": "packages/cli"
  },
  "homepage": "https://jeju.network",
  "bugs": {
    "url": "https://github.com/elizaos/jeju/issues"
  },
  "keywords": [
    "jeju",
    "cli",
    "blockchain",
    "ethereum",
    "l2",
    "optimism",
    "development",
    "testing",
    "deployment",
    "foundry",
    "forge"
  ],
  "publishConfig": {
    "access": "public"
  }
}
