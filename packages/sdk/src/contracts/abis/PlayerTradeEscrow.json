        { "name": "partyA", "type": "address", "indexed": true },
        { "name": "partyB", "type": "address", "indexed": true }
      ]
    },
    {
      "type": "event",
      "name": "TradeCancelled",
      "inputs": [
        { "name": "tradeId", "type": "uint256", "indexed": true },
        { "name": "cancelledBy", "type": "address", "indexed": true }
