{
  "name": "@jejunetwork/sdk",
  "version": "0.1.0",
  "type": "module",
  "description": "Network SDK - Full protocol access for compute, storage, DeFi, governance, and cross-chain",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "bun build.ts",
    "dev": "bun --watch build.ts",
    "test": "bun test test/*.test.ts test/e2e/*.test.ts",
    "test:unit": "bun test test/client.test.ts",
    "test:integration": "JEJU_NETWORK=localnet bun test test/integration",
    "test:integration:setup": "JEJU_NETWORK=localnet bun test/run-integration-tests.ts --setup",
    "test:integration:module": "JEJU_NETWORK=localnet bun test/run-integration-tests.ts --module",
    "test:e2e": "bun test test/e2e",
    "test:all": "bun test",
    "test:live": "START_SERVICES=1 JEJU_NETWORK=localnet bun test test/integration/full-sdk.integration.test.ts",
    "lint": "prettier --write ./src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elizaos/jeju.git",
    "directory": "packages/sdk"
  },
  "keywords": [
    "jeju",
    "sdk",
    "ethereum",
    "l2",
    "defi",
    "compute",
    "storage",
    "governance"
  ],
  "dependencies": {
    "viem": "2.43.2",
    "permissionless": "0.2.34",
    "@jejunetwork/types": "workspace:*",
    "@jejunetwork/config": "workspace:*"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "5.9.3",
    "prettier": "3.5.3",
    "execa": "^9.5.2"
  },
  "peerDependencies": {
    "zod": ">=4.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
