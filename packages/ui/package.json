{
  "name": "@jejunetwork/ui",
  "version": "0.1.0",
  "type": "module",
  "description": "React hooks and components for Jeju SDK",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "bun build.ts",
    "dev": "bun --watch build.ts",
    "test": "bun test",
    "lint": "prettier --write ./src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elizaos/jeju.git",
    "directory": "packages/ui"
  },
  "keywords": [
    "jeju",
    "ui",
    "react",
    "hooks",
    "components",
    "web3"
  ],
  "dependencies": {
    "@jejunetwork/sdk": "workspace:*",
    "@jejunetwork/types": "workspace:*",
    "viem": "2.43.2"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "zod": ">=4.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "19.2.3",
    "typescript": "5.9.3",
    "prettier": "3.5.3",
    "react": "19.2.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
