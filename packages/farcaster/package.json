{
  "name": "@jejunetwork/farcaster",
  "version": "0.1.0",
  "description": "Permissionless Farcaster integration for Jeju Network - No API keys required",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./hub": {
      "types": "./dist/hub/index.d.ts",
      "import": "./dist/hub/index.js"
    },
    "./frames": {
      "types": "./dist/frames/index.d.ts",
      "import": "./dist/frames/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elizaos/jeju.git",
    "directory": "packages/farcaster"
  },
  "keywords": [
    "farcaster",
    "hub",
    "social",
    "decentralized",
    "jeju",
    "permissionless",
    "frames"
  ],
  "dependencies": {
    "@farcaster/hub-nodejs": "0.12.0",
    "viem": "2.43.2",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "viem": ">=2.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
