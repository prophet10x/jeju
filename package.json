  "scripts": {
    "preinstall": "python3 -c 'import distutils' 2>/dev/null || (echo 'Python distutils not found - run: pip3 install --user --break-system-packages setuptools' && exit 1)",
    "docs:generate": "typedoc --options packages/config/typedoc.json || echo 'Docs generation skipped (optional)'",
    "postinstall": "bun run build:packages && bun run packages/deployment/scripts/setup-apps.ts || echo 'Setup completed with warnings'",
    "build:packages": "cd packages/types && bun run build && cd ../contracts && bun run build || echo 'Package build completed'",
    "build:cli": "cd packages/cli && bun run build",
    "forge": "cd packages/contracts && forge",
    "forge:test": "cd packages/contracts && forge test",
    "forge:build": "cd packages/contracts && forge build",
    "jeju": "bun run packages/cli/src/index.ts",
    "preflight": "bun run jeju -- deploy preflight",
    "dev": "bun run jeju -- dev",
    "start": "bun run jeju -- dev",
    "dev:vendor": "bun run jeju -- dev --vendor-only",
    "wallet": "bun run jeju -- keys",
    "ports": "bun run jeju -- ports",
    "cleanup": "bun run jeju -- cleanup",
    "build": "bun run docs:generate && bun run jeju -- build",
    "lint": "biome check packages apps",
    "lint:fix": "biome check --write packages apps",
    "typecheck": "turbo run typecheck --filter='!./vendor/*' --filter='!./vendor/*/packages/*' --filter='!./vendor/*/apps/*'",
    "typecheck:root": "tsc --noEmit",
    "test": "bun run packages/cli/src/index.ts test apps --no-docker",
    "test:core": "bun run packages/cli/src/index.ts test --phase=unit",
    "test:packages": "bun run packages/cli/src/index.ts test packages",
    "test:apps": "bun run packages/cli/src/index.ts test apps",
    "test:contracts": "bun run packages/cli/src/index.ts test --phase=contracts",
    "test:wallet": "bun run packages/cli/src/index.ts test --phase=wallet",
    "test:all": "bun run packages/cli/src/index.ts test",
    "test:help": "bun run packages/cli/src/index.ts test list",
    "test:load": "cd apps/gateway && bun test tests/load/pool-api-load.test.ts",
    "test:e2e": "bun run jeju -- test --mode=e2e",
    "test:e2e:smoke": "bun run jeju -- test --mode=smoke",
    "test:e2e:list": "bun run jeju -- test list",
    "test:e2e:help": "bun run jeju -- test list",
    "test:e2e:full": "bun run jeju -- test",
    "test:e2e:full:ci": "bun run jeju -- test --ci",
    "test:e2e:full:app": "bun run jeju -- test --app",
    "test:e2e:wallet": "bun run jeju -- test --app=wallet",
    "test:smoke": "cd packages/tests && bun run smoke:chain",
    "test:smoke:wallet": "cd packages/tests && bun run smoke:wallet",
    "test:preflight": "cd packages/tests && bun run preflight",
    "synpress:cache": "cd packages/tests && bun run synpress:cache",
    "synpress:cache:clear": "rm -rf .jeju/.synpress-cache",
    "clean": "bun run jeju -- clean",
    "localnet:start": "cd packages/deployment && bun run localnet:start",
    "localnet:stop": "cd packages/deployment && bun run localnet:stop",
    "localnet:reset": "cd packages/deployment && bun run localnet:reset",
    "infra:validate": "cd packages/deployment && bun run validate",
    "infra:plan": "cd packages/deployment && bun run infra:plan",
    "infra:apply": "cd packages/deployment && bun run infra:apply",
    "images:build": "cd packages/deployment && bun run images:build",
    "images:push": "cd packages/deployment && bun run images:push",
    "k8s:deploy": "cd packages/deployment && bun run k8s:deploy",
    "k8s:diff": "cd packages/deployment && bun run k8s:diff",
    "deploy:testnet": "cd packages/deployment && bun run deploy:testnet",
    "deploy:mainnet": "cd packages/deployment && bun run deploy:mainnet",
    "contracts:deploy:testnet": "bun run jeju -- deploy testnet",
    "contracts:deploy:mainnet": "bun run jeju -- deploy mainnet",
    "contracts:deploy": "bun run jeju -- deploy testnet",
    "oif:deploy:local": "bun run packages/deployment/scripts/deploy/oif.ts localnet",
    "oif:deploy:testnet": "bun run packages/deployment/scripts/deploy/oif.ts testnet",
    "oif:deploy:mainnet": "bun run packages/deployment/scripts/deploy/oif.ts mainnet",
    "oif:verify": "bun run jeju -- deploy verify oif",
    "testnet:check": "bun run jeju -- deploy check testnet",
    "testnet:keys": "bun run jeju -- deploy keys --network testnet",
    "testnet:full": "bun run jeju -- deploy testnet-full",
    "testnet:dws": "bun run packages/deployment/scripts/deploy/testnet-dws-infrastructure.ts",
    "testnet:frontends": "bun run packages/deployment/scripts/deploy/upload-frontends.ts testnet",
    "testnet:jns": "bun run packages/deployment/scripts/deploy/register-jns.ts testnet",
    "dapp:deploy": "bun run jeju -- deploy app --all",
    "dapp:deploy:app": "bun run jeju -- deploy app",
    "testnet:verify": "bun run packages/deployment/scripts/deploy/verify-testnet.ts",
    "testnet:self-host": "bun run packages/deployment/scripts/deploy/self-host-bootstrap.ts testnet",
    "testnet:deploy": "bun run jeju -- deploy testnet-full",
    "testnet:deployer": "bun run jeju -- keys setup-testnet",
    "testnet:fund": "bun run jeju -- fund --testnet",
    "testnet:bridge": "bun run jeju -- fund --testnet --bridge",
    "apps:list": "bun run jeju -- apps",
    "vendor:list": "bun run jeju -- apps",
    "vendor:sync": "bun run packages/deployment/scripts/setup-apps.ts",
    "jeju:dev": "bun run packages/cli/src/index.ts dev",
    "jeju:test": "bun run packages/cli/src/index.ts test",
    "jeju:deploy": "bun run packages/cli/src/index.ts deploy",
    "jeju:keys": "bun run packages/cli/src/index.ts keys",
    "jeju:status": "bun run packages/cli/src/index.ts status",
    "jeju:fund": "bun run packages/cli/src/index.ts fund",
    "status": "bun run packages/cli/src/index.ts status",
    "keys": "bun run packages/cli/src/index.ts keys",
    "init": "bun run packages/cli/src/index.ts init",
    "deploy": "bun run packages/cli/src/index.ts deploy",
    "publish:packages": "bun run jeju -- publish",
    "publish:packages:dry": "bun run jeju -- publish --dry-run",
    "link:packages": "cd packages/types && bun link && cd ../config && bun link && cd ../contracts && bun link && cd ../cli && bun link",
    "deps:check": "bunx depcheck --ignores='@playwright/test,@synthetixio/synpress,knip,madge,depcheck,playwright,playwright-core,prettier,prettier-plugin-solidity,solhint,turbo,typedoc-plugin-markdown,bun,bun:test,@jejunetwork/*,@babylon/*,ethers,@aws-sdk/*,@google-cloud/*,prom-client,hono,@eth-optimism/*,@hono/*,@noble/*,@opentelemetry/*,@openzeppelin/*,@safe-global/*,@synthetixio/*,@uniswap/*,ioredis,lru-cache,pg,tar,@types/pg,@types/ws,vitepress,ws,yaml-lint' --ignore-patterns='vendor/**,8004/**,**/node_modules/**,packages/contracts/lib/**'",
    "deps:circular": "bunx madge --circular --extensions ts packages/ --exclude 'lib/account-abstraction'",
    "deps:analyze": "bunx knip",
    "deps:analyze:ci": "bunx knip --no-exit-code",
    "ci:deps": "bun run deps:circular && bun run deps:check",
    "ci:lint": "bun run lint && bun run typecheck:root",
    "ci:test": "bun run test:core",
    "ci:all": "bun run ci:lint && bun run ci:deps && bun run ci:test"
  },
