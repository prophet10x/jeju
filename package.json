{
  "name": "jeju",
  "version": "0.1.0",
  "description": "A modern EVM chain for agents and humans",
  "type": "module",
  "workspaces": [
    "packages/*",
    "apps/*",
    "apps/*/app",
    "vendor/*",
    "vendor/*/packages/*",
    "vendor/*/apps/*"
  ],
  "overrides": {
    "viem": "2.41.2",
    "qs": ">=6.13.0",
    "express": "5.0.1",
