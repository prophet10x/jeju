{
  "name": "jeju",
  "version": "0.1.0",
  "description": "A modern EVM chain for agents and humans",
  "type": "module",
  "workspaces": [
    "packages/*",
    "apps/*",
    "apps/*/app",
    "apps/storage/da-server",
    "apps/storage/pinning-api",
    "apps/storage/cache-service",
    "vendor/*",
    "vendor/*/packages/*",
    "vendor/*/apps/*"
  ],
  "overrides": {
    "@jejunetwork/cli": "workspace:*",
    "@jejunetwork/contracts": "workspace:*",
    "@jejunetwork/types": "workspace:*",
    "@jejunetwork/tests": "workspace:*",
    "@jejunetwork/config": "workspace:*",
    "qs": ">=6.13.0",
    "express": ">=4.21.0",
    "ws": ">=8.18.0",
    "body-parser": ">=1.20.3",
    "cookie": ">=0.7.0",
    "path-to-regexp": ">=0.1.10",
    "send": ">=0.19.0",
    "fresh": ">=0.5.2",
    "debug": ">=2.6.9",
    "socket.io-parser": ">=4.2.4",
    "nanoid": ">=5.0.9",
    "form-data": ">=4.0.0",
    "jsondiffpatch": ">=0.7.2",
    "engine.io-client": ">=6.6.2",
    "parse-duration": ">=2.1.0",
    "tmp": ">=0.2.3",
    "undici": ">=6.21.0",
    "esbuild": ">=0.25.0",
    "mime": ">=4.0.0",
    "morgan": ">=1.10.0",
    "uglify-js": ">=3.19.0",
    "next": ">=15.1.0",
    "@hapi/hoek": ">=9.0.3",
    "negotiator": ">=0.6.4",
    "axios": ">=1.7.4",
    "hono": ">=4.6.10",
    "js-yaml": ">=4.0.0",
    "socket.io": ">=4.8.0",
    "glob": ">=11.0.0",
    "hawk": ">=9.0.1",
    "playwright": ">=1.55.1",
    "ai": ">=5.0.52",
    "got": ">=11.8.5",
    "tunnel-agent": ">=0.6.0",
    "tsup": ">=8.5.1",
    "tough-cookie": ">=4.1.3",
    "node-fetch": ">=2.6.7"
  },
  "scripts": {
    "preinstall": "python3 -c 'import distutils' 2>/dev/null || (echo '\\n⚠️  Python distutils required\\n\\nFix: pip3 install --user --break-system-packages setuptools\\n' && exit 1)",
    "postinstall": "bun run build:packages && bun run scripts/setup-apps.ts || echo 'Setup completed'",
    "build:packages": "cd packages/types && bun run build && cd ../contracts && bun run build || echo 'Done'",

    "jeju": "bun run packages/cli/src/index.ts",
    "dev": "bun run packages/cli/src/index.ts dev",
    "dev:decentralized": "bun run packages/cli/src/index.ts dev --decentralized",
    "start:decentralized": "bun run scripts/start-decentralized.ts",
    "test": "bun run packages/cli/src/index.ts test",
    "deploy": "bun run packages/cli/src/index.ts deploy",
    "status": "bun run packages/cli/src/index.ts status",
    "keys": "bun run packages/cli/src/index.ts keys",
    "init": "bun run packages/cli/src/index.ts init",

    "localnet:start": "cd packages/deployment && bun run localnet:start",
    "localnet:stop": "cd packages/deployment && bun run localnet:stop",
    "localnet:reset": "cd packages/deployment && bun run localnet:reset",

    "build": "turbo run build",
    "lint": "eslint packages scripts --ext ts",
    "clean": "bun run scripts/clean.ts",

    "test:contracts": "cd packages/contracts && forge test",
    "test:e2e": "cd packages/tests && bun run e2e",
    "test:smoke": "cd packages/tests && bun run smoke:chain",

    "docs:generate": "typedoc --options packages/config/typedoc.json || echo 'Skipped'"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.911.0",
    "@eth-optimism/sdk": "^3.2.0",
    "@noble/curves": "^2.0.1",
    "@openzeppelin/contracts": "^5.0.2",
    "@safe-global/safe-core-sdk": "^3.3.5",
    "@synthetixio/v3-contracts": "latest",
    "@uniswap/v4-core": "latest",
    "ethers": "^6.11.1",
    "merkletreejs": "^0.6.0",
    "tar": "^7.5.1",
    "viem": "^2.7.15",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@account-abstraction/contracts": "^0.8.0",
    "@playwright/test": "^1.56.1",
    "@shikijs/engine-javascript": "^3.20.0",
    "@shikijs/transformers": "^3.20.0",
    "@synthetixio/synpress": "^4.0.0-alpha.7",
    "@types/bun": "latest",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "next": "16.0.10",
    "playwright": ">=1.55.1",
    "playwright-core": "^1.56.1",
    "prettier": "^3.2.5",
    "prettier-plugin-solidity": "^2.1.0",
    "solhint": "^6.0.1",
    "turbo": "^2.6.3",
    "typedoc": "^0.28.14",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.3.3",
    "vitepress": "^1.0.0",
    "ws": "^8.18.3",
    "yaml-lint": "^1.7.0"
  },
  "resolutions": {
    "qs": ">=6.13.0",
    "express": ">=4.21.0",
    "ws": ">=8.18.0",
    "body-parser": ">=1.20.3",
    "cookie": ">=0.7.0",
    "path-to-regexp": ">=0.1.10",
    "send": ">=0.19.0",
    "fresh": ">=0.5.2",
    "debug": ">=2.6.9",
    "socket.io-parser": ">=4.2.4",
    "nanoid": ">=5.0.9",
    "@hapi/hoek": ">=9.0.3",
    "hawk": ">=9.0.1",
    "playwright": ">=1.55.1",
    "tough-cookie": ">=4.1.3",
    "node-fetch": ">=2.6.7"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2022": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "ignorePatterns": [
      "node_modules",
      "dist",
      "build",
      "out",
      "lib",
      ".next",
      ".cache",
      "apps",
      "babylon",
      "caliguland2",
      "packages/contracts",
      "packages/deployment/kubernetes",
      "packages/deployment/terraform",
      "packages/deployment/kurtosis",
      "*.config.js",
      "*.config.ts"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": ["warn", {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }],
      "@typescript-eslint/no-explicit-any": "warn",
      "@typescript-eslint/no-var-requires": "off",
      "no-console": "off"
    }
  }
}
