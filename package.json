    "tunnel-agent": ">=0.6.0",
    "tsup": ">=8.5.1",
    "tough-cookie": ">=4.1.3"
  },
  "scripts": {
    "preinstall": "python3 -c 'import distutils' 2>/dev/null || (echo '\\n⚠️  Python distutils not found (required for native modules like better-sqlite3)\\n\\nFix with: pip3 install --user --break-system-packages setuptools\\n' && exit 1)",
    "docs:generate": "typedoc --options packages/config/typedoc.json || echo 'Docs generation skipped (optional)'",
    "postinstall": "bun run build:packages && bun run scripts/setup-apps.ts || echo 'Setup completed with warnings'",
    "build:packages": "cd packages/types && bun run build && cd ../contracts && bun run build || echo 'Package build completed'",
    "build:cli": "cd packages/cli && bun run build",
    "forge": "cd packages/contracts && forge",
    "forge:test": "cd packages/contracts && forge test",
    "forge:build": "cd packages/contracts && forge build",
    "jeju": "bun run packages/cli/src/index.ts",
    "preflight": "jeju deploy preflight",
    "dev": "jeju dev",
    "start": "jeju dev",
    "dev:vendor": "jeju dev --vendor-only",
    "cql": "docker compose up -d cql",
    "cql:stop": "docker compose down cql",
    "cql:logs": "docker logs jeju-cql -f",
    "wallet": "jeju keys",
    "ports": "jeju ports",
    "cleanup": "jeju cleanup",
    "build": "bun run docs:generate && jeju build",
    "lint": "eslint scripts --ext ts",
    "test": "bun run packages/cli/src/index.ts test apps --no-docker",
    "test:core": "bun run packages/cli/src/index.ts test --phase=unit",
    "test:packages": "bun run packages/cli/src/index.ts test packages",
    "test:apps": "bun run packages/cli/src/index.ts test apps",
    "test:contracts": "bun run packages/cli/src/index.ts test --phase=contracts",
    "test:wallet": "bun run packages/cli/src/index.ts test --phase=wallet",
    "test:all": "bun run packages/cli/src/index.ts test",
    "test:help": "bun run packages/cli/src/index.ts test list",
    "test:load": "cd apps/gateway && bun test tests/load/pool-api-load.test.ts",
    "test:e2e": "jeju test --mode=e2e",
    "test:e2e:smoke": "jeju test --mode=smoke",
    "test:e2e:list": "jeju test list",
    "test:e2e:help": "jeju test list",
    "test:e2e:full": "jeju test",
    "test:e2e:full:ci": "jeju test --ci",
    "test:e2e:full:app": "jeju test --app",
    "test:e2e:wallet": "jeju test --app=wallet",
    "test:smoke": "cd packages/tests && bun run smoke:chain",
    "test:smoke:wallet": "cd packages/tests && bun run smoke:wallet",
    "test:preflight": "cd packages/tests && bun run preflight",
    "verify:monitoring": "echo 'Monitoring verification removed - use jeju status'",
    "verify:rollback": "echo 'Rollback verification removed - use jeju deploy rollback'",
    "verify:security": "echo 'Security verification removed'",
    "synpress:cache": "cd packages/tests && bun run synpress:cache",
    "synpress:cache:clear": "rm -rf .jeju/.synpress-cache",
    "clean": "jeju clean",
    "localnet:start": "cd packages/deployment && bun run localnet:start",
    "localnet:stop": "cd packages/deployment && bun run localnet:stop",
    "localnet:reset": "cd packages/deployment && bun run localnet:reset",
    "infra:validate": "cd packages/deployment && bun run validate",
    "infra:plan": "cd packages/deployment && bun run infra:plan",
    "infra:apply": "cd packages/deployment && bun run infra:apply",
    "images:build": "cd packages/deployment && bun run images:build",
    "images:push": "cd packages/deployment && bun run images:push",
    "k8s:deploy": "cd packages/deployment && bun run k8s:deploy",
    "k8s:diff": "cd packages/deployment && bun run k8s:diff",
    "deploy:testnet": "cd packages/deployment && bun run deploy:testnet",
    "deploy:mainnet": "cd packages/deployment && bun run deploy:mainnet",
    "contracts:deploy:testnet": "bun run scripts/deploy/testnet.ts",
    "contracts:deploy:mainnet": "bun run scripts/deploy/mainnet.ts",
    "contracts:deploy": "bun run scripts/deploy/testnet.ts",
    "oif:deploy:local": "bun run scripts/deploy/oif.ts localnet",
    "oif:deploy:testnet": "bun run scripts/deploy/oif.ts testnet",
    "oif:deploy:mainnet": "bun run scripts/deploy/oif.ts mainnet",
    "oif:verify": "jeju deploy verify oif",
    "testnet:check": "jeju deploy check testnet",
    "testnet:keys": "jeju deploy keys --network testnet",
    "testnet:full": "bun run scripts/deploy/testnet-full.ts",
    "testnet:deployer": "jeju keys setup-testnet",
    "testnet:fund": "jeju fund --testnet",
    "testnet:bridge": "jeju fund --testnet --bridge",
    "testnet:accumulate": "echo 'Token accumulator removed'",
    "testnet:accumulate:watch": "echo 'Token accumulator removed'",
    "apps:list": "jeju apps",
    "vendor:list": "jeju apps",
    "vendor:sync": "bun run scripts/setup-apps.ts",
    "vendor:update": "echo 'Vendor update removed - use git submodule update'",
    "jeju:dev": "bun run packages/cli/src/index.ts dev",
    "jeju:test": "bun run packages/cli/src/index.ts test",
    "jeju:deploy": "bun run packages/cli/src/index.ts deploy",
    "jeju:keys": "bun run packages/cli/src/index.ts keys",
    "jeju:status": "bun run packages/cli/src/index.ts status",
    "jeju:fund": "bun run packages/cli/src/index.ts fund",
    "status": "bun run packages/cli/src/index.ts status",
    "keys": "bun run packages/cli/src/index.ts keys",
    "init": "bun run packages/cli/src/index.ts init",
    "deploy": "bun run packages/cli/src/index.ts deploy",
    "publish:packages": "jeju publish",
    "publish:packages:dry": "jeju publish --dry-run",
    "link:packages": "cd packages/types && bun link && cd ../config && bun link && cd ../contracts && bun link && cd ../cli && bun link"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.911.0",
    "@eth-optimism/sdk": "^3.2.0",
    "@hono/node-server": "^1.19.7",
    "@noble/curves": "^2.0.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.67.2",
    "@opentelemetry/core": "^2.2.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.208.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-metrics": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/sdk-trace-base": "^2.2.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@openzeppelin/contracts": "^5.0.2",
    "@safe-global/safe-core-sdk": "^3.3.5",
    "@synthetixio/v3-contracts": "latest",
    "@uniswap/v4-core": "latest",
    "ioredis": "^5.8.2",
    "lru-cache": "^11.2.4",
    "merkletreejs": "^0.6.0",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "tar": "^7.5.1",
    "viem": "2.41.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@account-abstraction/contracts": "^0.8.0",
    "@playwright/test": "^1.56.1",
    "@shikijs/engine-javascript": "^3.20.0",
    "@shikijs/transformers": "^3.20.0",
    "@synthetixio/synpress": "^4.0.0-alpha.7",
    "@types/bun": "latest",
    "@types/pg": "^8.16.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "next": "16.0.10",
    "playwright": ">=1.55.1",
    "playwright-core": "^1.56.1",
    "prettier": "^3.2.5",
    "prettier-plugin-solidity": "^2.1.0",
    "solhint": "^6.0.1",
    "turbo": "^2.6.3",
    "typedoc": "^0.28.14",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.3.3",
    "vitepress": "^1.0.0",
    "ws": "^8.18.3",
    "yaml-lint": "^1.7.0"
  },
  "resolutions": {
    "viem": "2.41.2",
    "qs": ">=6.13.0",
    "express": "5.0.1",
    "node-fetch": "2.7.0",
    "ws": ">=8.18.0",
    "body-parser": ">=1.20.3",
    "cookie": ">=0.7.0",
    "path-to-regexp": ">=0.1.10",
    "send": ">=0.19.0",
    "fresh": ">=0.5.2",
    "debug": ">=2.6.9",
    "socket.io-parser": ">=4.2.4",
    "nanoid": ">=5.0.9",
    "@hapi/hoek": ">=9.0.3",
    "hawk": ">=9.0.1",
    "playwright": ">=1.55.1",
    "tough-cookie": ">=4.1.3"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2022": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "ignorePatterns": [
      "node_modules",
      "dist",
      "build",
      "out",
      "lib",
      ".next",
      ".cache",
      "apps",
      "babylon",
      "caliguland2",
      "packages/contracts",
      "packages/deployment/kubernetes",
      "packages/deployment/terraform",
      "packages/deployment/kurtosis",
      "*.config.js",
      "*.config.ts"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": ["warn", {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }],
      "@typescript-eslint/no-explicit-any": "warn",
      "@typescript-eslint/no-var-requires": "off",
      "no-console": "off"
    }
  },
  "patchedDependencies": {
    "@fastify/static@8.3.0": "patches/@fastify%2Fstatic@8.3.0.patch"
  }
}
