# Jeju Network - Environment Variables
# =====================================
# Copy to .env and fill in your values.
# NEVER commit .env to git.
#
# Jeju is 100% decentralized by default. All services use decentralized
# infrastructure (IPFS, CovenantSQL, Cache Service) with no fallbacks.
#
# Quick Start:
#   1. cp env.example .env
#   2. docker compose up -d
#   3. bun run dev

# ==============================================================================
# REQUIRED: Wallet
# ==============================================================================
# Generate: cast wallet new
# Anvil test key (localnet only):
PRIVATE_KEY=0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80
DEPLOYER_ADDRESS=0xf39Fd6e51aad88F6F4ce6aB8827279cffFb92266

# ==============================================================================
# Blockchain Configuration
# ==============================================================================
JEJU_NETWORK=localnet
JEJU_RPC_URL=http://localhost:9545
RPC_URL=http://localhost:9545
CHAIN_ID=1337

# ==============================================================================
# CovenantSQL (Decentralized Database)
# ==============================================================================
COVENANTSQL_NODES=http://localhost:4661
COVENANTSQL_DATABASE_ID=jeju-dev
COVENANTSQL_PRIVATE_KEY=0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80
COVENANTSQL_CONSISTENCY=strong
COVENANTSQL_POOL_SIZE=10
COVENANTSQL_TIMEOUT=30000
COVENANTSQL_RETRIES=3
COVENANTSQL_LOGGING=false

# ==============================================================================
# IPFS (Decentralized Storage)
# ==============================================================================
IPFS_API_URL=http://localhost:5001
IPFS_GATEWAY_URL=http://localhost:4180

# ==============================================================================
# Cache Service (Decentralized Cache)
# ==============================================================================
CACHE_SERVICE_URL=http://localhost:4115
CACHE_NAMESPACE=default
CACHE_DEFAULT_TTL=3600
CACHE_TIMEOUT=5000

# ==============================================================================
# DA Server / Vault (Data Availability)
# ==============================================================================
DA_SERVER_URL=http://localhost:4010
VAULT_ENDPOINT=http://localhost:4010
VAULT_ENCRYPTION_SECRET=dev-vault-secret-change-in-production
VAULT_AUDIT_LOGGING=true
USE_VAULT=true
ACCESSOR_ADDRESS=0xf39Fd6e51aad88F6F4ce6aB8827279cffFb92266

# ==============================================================================
# Compute / Trigger Service
# ==============================================================================
COMPUTE_ENDPOINT=http://localhost:4016
TRIGGER_SERVICE_URL=http://localhost:4016
TRIGGER_TIMEOUT=10000

# ==============================================================================
# JNS Gateway (Name Service)
# ==============================================================================
JNS_GATEWAY_URL=http://localhost:4005
JNS_REGISTRY_ADDRESS=0x0000000000000000000000000000000000000000
JNS_RESOLVER_ADDRESS=0x0000000000000000000000000000000000000000

# ==============================================================================
# KMS Configuration
# ==============================================================================
KMS_FALLBACK_SECRET=dev-kms-secret-change-in-production
KMS_DEFAULT_PROVIDER=encryption
KMS_DEBUG=false

# ==============================================================================
# TEE Configuration (Phala Cloud)
# ==============================================================================
# Leave TEE_ENDPOINT blank for local encrypted mode
# Set to your Phala CVM endpoint for production TEE
TEE_ENDPOINT=
TEE_ENCRYPTION_SECRET=dev-tee-secret-change-in-production

# ==============================================================================
# MPC Coordinator Configuration
# ==============================================================================
MPC_NETWORK=localnet
MPC_THRESHOLD=2
MPC_TOTAL_PARTIES=3
MPC_ENCRYPTION_SECRET=dev-mpc-secret-change-in-production

# ==============================================================================
# PostgreSQL (Subsquid Indexer Only)
# ==============================================================================
DATABASE_URL=postgresql://jeju:jeju_dev_password@localhost:5434/jeju
DATABASE_PASSWORD=jeju_dev_password

# ==============================================================================
# Auth & Sessions
# ==============================================================================
SESSION_SECRET=dev-session-secret-change-in-production
JWT_SECRET=dev-jwt-secret-change-in-production

# ==============================================================================
# OPTIONAL: API Keys
# ==============================================================================
# Contract verification (https://etherscan.io/myapikey)
ETHERSCAN_API_KEY=

# WalletConnect (https://cloud.walletconnect.com)
WALLETCONNECT_PROJECT_ID=

# AI features
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# ==============================================================================
# OPTIONAL: AWS (for testnet/mainnet deployment)
# ==============================================================================
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
