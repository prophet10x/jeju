{
  "name": "@jejunetwork/otto",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Otto - Decentralized multi-platform AI trading agent for Discord, Telegram, and WhatsApp",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "bun run src/server.ts",
    "dev:discord": "ENABLE_DISCORD=true bun run src/server.ts",
    "dev:telegram": "ENABLE_TELEGRAM=true bun run src/server.ts",
    "dev:all": "ENABLE_DISCORD=true ENABLE_TELEGRAM=true ENABLE_WHATSAPP=true bun run src/server.ts",
    "test": "bun test src/",
    "test:watch": "bun test --watch src/",
    "test:integration": "INTEGRATION=true bun test src/tests/integration.test.ts",
    "test:ui": "npx playwright test -c playwright.config.ts",
    "test:wallet": "npx playwright test -c synpress.config.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist out cache test-results"
  },
  "dependencies": {
    "@elizaos/core": "^1.0.0",
    "@elizaos/adapter-sqlite": "0.25.6-alpha.1",
    "@elizaos/plugin-twitter": "^1.0.0",
    "@elizaos/plugin-discord": "^1.0.0",
    "@elizaos/plugin-telegram": "^1.0.0",
    "@elysiajs/cors": "^1.3.1",
    "@jejunetwork/config": "workspace:*",
    "@jejunetwork/db": "workspace:*",
    "@jejunetwork/shared": "workspace:*",
    "@jejunetwork/types": "workspace:*",
    "@jejunetwork/sdk": "workspace:*",
    "@jejunetwork/kms": "workspace:*",
    "@jejunetwork/oauth3": "workspace:*",
    "@solana/web3.js": "1.98.4",
    "discord.js": "14.16.3",
    "elysia": "^1.3.1",
    "telegraf": "4.16.3",
    "twilio": "5.4.0",
    "viem": "2.43.2",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@jejunetwork/tests": "workspace:*",
    "@playwright/test": "1.57.0",
    "@synthetixio/synpress": "4.1.1",
    "@types/bun": "latest",
    "typescript": "5.9.3"
  }
}
