{
  "name": "@jejunetwork/crucible",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Decentralized agent orchestration platform - autonomous AI agents with serverless compute, storage, and coordination",
  "main": "api/index.ts",
  "scripts": {
    "dev": "concurrently \"bun run scripts/dev.ts\" \"API_PORT=${API_PORT:-4021} bun --watch api/server.ts\"",
    "dev:server": "bun run api/server.ts",
    "build": "bun run scripts/build.ts",
    "start": "bun run scripts/serve.ts",
    "test": "bun test tests/ && bunx playwright test --config synpress.config.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@elizaos/core": "^1.6.6",
    "@elysiajs/cors": "^1.3.1",
    "@jejunetwork/config": "workspace:*",
    "@jejunetwork/shared": "workspace:*",
    "@jejunetwork/sdk": "workspace:*",
    "@jejunetwork/types": "workspace:*",
    "@jejunetwork/eliza-plugin": "workspace:*",
    "@tanstack/react-query": "5.90.12",
    "concurrently": "^9.1.2",
    "elysia": "^1.3.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router-dom": "^7.1.1",
    "sonner": "1.7.2",
    "viem": "2.43.3",
    "zod": "4.2.1"
  },
  "devDependencies": {
    "@jejunetwork/tests": "workspace:*",
    "@playwright/test": "1.57.0",
    "@synthetixio/synpress": "4.1.1",
    "@types/bun": "latest",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.0",
    "autoprefixer": "10.4.21",
    "postcss": "8.5.3",
    "tailwindcss": "3.4.17",
    "typescript": "5.9.3"
  }
}
