{
  "name": "@jejunetwork/autocrat-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --port=3010 ./index.html",
    "build": "rm -rf dist && bun build ./index.html --outdir dist --minify && bunx tailwindcss -i src/app/globals.css -o dist/index.css --minify && bun run fix-css-path",
    "fix-css-path": "bun -e \"const html = Bun.file('dist/index.html'); const text = await html.text(); await Bun.write('dist/index.html', text.replace(/index-[a-z0-9]+\\.css/, 'index.css'))\"",
    "start": "bun run server.ts",
    "lint": "biome check .",
    "test": "playwright test --project=chromium",
    "test:all": "playwright test",
    "test:ui": "playwright test --ui"
  },
  "dependencies": {
    "@rainbow-me/rainbowkit": "2.2.9",
    "@tanstack/react-query": "5.90.12",
    "lucide-react": "0.562.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router-dom": "7.1.1",
    "sonner": "1.7.2",
    "viem": "2.43.2",
    "wagmi": "2.18.1"
  },
  "devDependencies": {
    "@playwright/test": "1.57.0",
    "@types/bun": "1.2.10",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "autoprefixer": "10.4.21",
    "bun-plugin-tailwind": "0.1.2",
    "postcss": "8.5.3",
    "tailwindcss": "3.4.17",
    "typescript": "5.9.3"
  }
}
