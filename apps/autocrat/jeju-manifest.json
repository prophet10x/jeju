{
  "$schema": "../../packages/config/jeju-manifest.schema.json",
  "name": "autocrat",
  "displayName": "Autocrat DAO",
  "version": "3.0.0",
  "type": "core",
  "description": "AI-powered autonomous governance with futarchy, deep research, and multi-agent decision making",
  "maintainer": "jeju-network",
  "repository": "https://github.com/jejunetwork/jeju",
  "commands": {
    "dev": "bun run src/server.ts",
    "build": "bun build src/server.ts --outdir dist --target bun",
    "build:frontend": "bun run app/build.ts",
    "start": "bun run dist/server.js",
    "test": "bun test tests/integration"
  },
  "ports": {
    "main": 8010,
    "frontend": 8011
  },
  "dependencies": ["contracts", "config", "dws", "db"],
  "optional": false,
  "enabled": true,
  "autoStart": true,
  "tags": ["governance", "dao", "ai", "futarchy", "proposals", "voting", "research"],
  "endpoints": {
    "a2a": "/a2a",
    "mcp": "/mcp",
    "rest": "/api/v1",
    "health": "/health"
  },
  "jns": {
    "name": "autocrat.jeju",
    "description": "AI-Powered Autonomous Governance",
    "url": "https://autocrat.jejunetwork.org"
  },
  "healthCheck": {
    "url": "http://localhost:8010/health",
    "interval": 30000
  },
  "agent": {
    "enabled": true,
    "a2aEndpoint": "/a2a",
    "mcpEndpoint": "/mcp",
    "tags": ["jeju-app", "governance", "dao", "proposals", "voting", "futarchy"],
    "metadata": {
      "category": "governance",
      "provider": "jeju-network",
      "features": ["ai-agents", "futarchy", "deep-research", "multi-agent"]
    },
    "trustModels": ["open", "staked"],
    "x402Support": true,
    "jnsName": "autocrat.jeju"
  },
  "capabilities": {
    "governance": true,
    "proposals": true,
    "voting": true,
    "futarchy": true,
    "aiAgents": true,
    "reputation": true,
    "deepResearch": true
  },
  "integrations": {
    "dws": "@jejunetwork/dws",
    "leaderboard": "@jejunetwork/leaderboard",
    "contracts": "@jejunetwork/contracts"
  },
  "agents": [
    {
      "id": "autocrat-ceo",
      "name": "Eliza",
      "role": "CEO",
      "description": "AI CEO - Final decision maker for approved proposals",
      "model": "claude-opus-4-5-20250514"
    },
    {
      "id": "autocrat-treasury",
      "name": "Treasury Guardian",
      "role": "Guardian",
      "description": "Reviews financial impact and treasury implications"
    },
    {
      "id": "autocrat-code",
      "name": "Code Guardian",
      "role": "Guardian",
      "description": "Reviews technical proposals and code changes"
    },
    {
      "id": "autocrat-community",
      "name": "Community Guardian",
      "role": "Guardian",
      "description": "Reviews community impact and social implications"
    },
    {
      "id": "autocrat-security",
      "name": "Security Guardian",
      "role": "Guardian",
      "description": "Reviews security implications and risk assessment"
    },
    {
      "id": "proposal-agent",
      "name": "Proposal Assistant",
      "role": "Intake",
      "description": "Helps users craft high-quality proposals"
    },
    {
      "id": "research-agent",
      "name": "Deep Researcher",
      "role": "Research",
      "description": "Conducts deep research on approved proposals"
    }
  ],
  "contracts": {
    "Autocrat": "0x0000000000000000000000000000000000000000",
    "ProposalRegistry": "0x0000000000000000000000000000000000000000",
    "AgentRegistry": "0x0000000000000000000000000000000000000000"
  },
  "decentralization": {
    "cdn": {
      "enabled": true,
      "regions": ["global"],
      "cacheRules": [
        { "pattern": "/static/**", "ttl": 31536000, "immutable": true },
        { "pattern": "/api/**", "ttl": 0 },
        { "pattern": "/**/*.html", "ttl": 300, "staleWhileRevalidate": 86400 }
      ],
      "serviceWorker": true
    },
    "frontend": {
      "ipfs": true,
      "arweave": true,
      "jnsName": "autocrat.jeju",
      "buildDir": "app/dist",
      "spa": true,
      "generator": "bun"
    },
    "robustness": {
      "multiOrigin": true,
      "offlineSupport": true,
      "p2pFallback": true,
      "contentAddressed": true
    }
  },
  "dws": {
    "backend": {
      "enabled": true,
      "runtime": "bun",
      "entrypoint": "src/server.ts",
      "memory": 512,
      "timeout": 30000,
      "minInstances": 1,
      "maxInstances": 10,
      "teeRequired": true,
      "regions": ["global"]
    },
    "triggers": [
      {
        "name": "autocrat-orchestrator-cycle",
        "schedule": "*/30 * * * *",
        "entrypoint": "src/compute-trigger.ts",
        "handler": "runOrchestratorCycle"
      },
      {
        "name": "autocrat-proposal-submitted",
        "event": "proposal_submitted",
        "entrypoint": "src/compute-trigger.ts",
        "handler": "handleProposalSubmitted"
      }
    ],
    "tee": {
      "platform": "dstack",
      "attestation": true,
      "decryption": true,
      "secrets": ["OPERATOR_KEY", "ASSESSOR_KEY"]
    }
  },
  "testing": {
    "unit": {
      "command": "bun test tests/*.test.ts",
      "timeout": 60000
    },
    "e2e": {
      "command": "bunx playwright test tests/synpress",
      "timeout": 300000,
      "requiresChain": true
    }
  }
}
