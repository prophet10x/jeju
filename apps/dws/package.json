{
  "name": "@jejunetwork/dws",
  "version": "1.0.0",
  "description": "Decentralized Web Services (storage + compute + cdn)",
  "type": "module",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./sdk": "./src/sdk/index.ts",
    "./cdn": "./src/cdn/index.ts",
    "./cdn/edge": "./src/cdn/edge/index.ts",
    "./cdn/sdk": "./src/cdn/sdk/index.ts",
    "./training": "./src/training/index.ts"
  },
  "scripts": {
    "postinstall": "bun run scripts/install-workerd.ts",
    "install:workerd": "bun run scripts/install-workerd.ts",
    "dev": "bun run scripts/dev.ts",
    "dev:server": "NETWORK=localnet RPC_URL=http://127.0.0.1:6546 bun run src/server/index.ts",
    "dev:local": "bun run scripts/local-dev.ts",
    "dev:local:verbose": "bun run scripts/local-dev.ts --verbose",
    "dev:frontend": "cd frontend && bun run dev",
    "dev:all": "bun run dev & cd frontend && bun run dev",
    "start": "bun run src/server/index.ts",
    "node": "bun run src/node/index.ts",
    "inference": "bun run src/compute/local-inference-server.ts",
    "test": "NODE_ENV=test bun test src/tests src/cdn/tests src/agents src/email/tests src/poc/tests src/training tests/*.test.ts tests/integration/*.test.ts tests/storage/*.test.ts",
    "test:workerd": "NODE_ENV=test bun test tests/workerd.test.ts",
    "test:integration": "bun test tests/*.test.ts tests/integration/*.test.ts",
    "test:unit": "bun test src/tests src/cdn/tests",
    "test:frontend": "cd frontend && bunx playwright test",
    "test:e2e": "bun run scripts/e2e-setup.ts",
    "test:e2e:real": "E2E_MODE=true DWS_URL=http://localhost:4030 RPC_URL=http://127.0.0.1:6546 bun test tests/real-e2e.test.ts",
    "test:e2e:full": "NETWORK=localnet bun test tests/full-e2e.test.ts",
    "test:synpress": "cd frontend && bunx synpress --headless",
    "train:demo": "bun run src/training/demo-full-system.ts",
    "train:integrated": "bun run src/training/run-integrated-demo.ts",
    "train:real": "bun run src/training/run-real-training.ts",
    "train:benchmark": "bun run src/training/benchmark-tictactoe.ts",
    "train:test": "bun test src/training/",
    "train:atropos": "bun run src/training/atropos-server.ts",
    "train:api": "bun run src/training/dws-integration.ts",
    "test:all": "bun test src/tests src/cdn/tests tests/*.test.ts tests/integration/*.test.ts",
    "typecheck": "tsc --noEmit && cd frontend && bun run typecheck",
    "cdn:edge": "bun run src/cdn/edge/index.ts",
    "cdn:coordinator": "bun run src/cdn/routing/coordinator.ts",
    "cdn:gateway": "bun run src/cdn/gateway/jns-gateway.ts",
    "proxy:coordinator": "bun run src/proxy/scripts/start-coordinator.ts",
    "proxy:node": "bun run src/proxy/scripts/start-node.ts",
    "triggers": "bun run src/triggers/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "build:frontend": "cd frontend && bun run build",
    "seed": "bun run scripts/seed-dev.ts"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.954.0",
    "@jejunetwork/config": "workspace:*",
    "@jejunetwork/db": "workspace:*",
    "@jejunetwork/sdk": "workspace:*",
    "@jejunetwork/shared": "workspace:*",
    "@jejunetwork/types": "workspace:*",
    "@noble/curves": "^1.9.7",
    "@noble/hashes": "^2.0.1",
    "c-kzg": "^4.1.0",
    "croner": "^9.0.0",
    "hono": "4.7.0",
    "kzg-wasm": "^1.0.0",
    "lru-cache": "11.2.4",
    "opossum": "^9.0.0",
    "p-retry": "^7.1.1",
    "prom-client": "^15.1.3",
    "tweetnacl": "^1.0.3",
    "viem": "2.43.2",
    "yaml": "^2.8.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/bun": "latest"
  }
}
