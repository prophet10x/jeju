{
  "name": "@jejunetwork/dws",
  "version": "1.0.0",
  "description": "Decentralized Web Services (storage + compute + cdn)",
  "type": "module",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./sdk": "./src/sdk/index.ts",
    "./cdn": "./src/cdn/index.ts",
    "./cdn/edge": "./src/cdn/edge/index.ts",
    "./cdn/sdk": "./src/cdn/sdk/index.ts"
  },
  "scripts": {
    "dev": "bun run scripts/dev.ts",
    "dev:server": "bun run src/server/index.ts",
    "dev:frontend": "cd frontend && bun run dev",
    "dev:all": "bun run dev & cd frontend && bun run dev",
    "start": "bun run src/server/index.ts",
    "node": "bun run src/node/index.ts",
    "test": "NODE_ENV=test bun test src/tests src/cdn/tests",
    "test:integration": "bun test src/tests src/cdn/tests tests/*.test.ts tests/integration",
    "test:frontend": "cd frontend && bun run test",
    "test:e2e": "cd frontend && bun run test:headed",
    "test:all": "bun test src/tests src/cdn/tests tests/*.test.ts tests/integration",
    "typecheck": "tsc --noEmit && cd frontend && bun run typecheck",
    "cdn:edge": "bun run src/cdn/edge/index.ts",
    "cdn:coordinator": "bun run src/cdn/routing/coordinator.ts",
    "cdn:gateway": "bun run src/cdn/gateway/jns-gateway.ts",
    "proxy:coordinator": "bun run src/proxy/scripts/start-coordinator.ts",
    "proxy:node": "bun run src/proxy/scripts/start-node.ts",
    "triggers": "bun run src/triggers/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "build:frontend": "cd frontend && bun run build",
    "seed": "bun run scripts/seed-dev.ts"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.954.0",
    "@jejunetwork/config": "workspace:*",
    "@jejunetwork/db": "workspace:*",
    "@jejunetwork/sdk": "workspace:*",
    "@jejunetwork/shared": "workspace:*",
    "@jejunetwork/types": "workspace:*",
    "hono": "4.7.0",
    "lru-cache": "11.2.4",
    "viem": "2.41.2"
  },
  "devDependencies": {
    "@types/bun": "latest"
  }
}
