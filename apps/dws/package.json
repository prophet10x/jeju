{
  "name": "@jejunetwork/dws",
  "version": "1.0.0",
  "description": "Decentralized Web Services - storage, compute, CDN, training, and infrastructure",
  "type": "module",
  "main": "api/index.ts",
  "exports": {
    ".": "./api/index.ts"
  },
  "scripts": {
    "dev": "concurrently \"bun run scripts/dev-frontend.ts\" \"bun run api/server/index.ts\"",
    "dev:old": "concurrently \"PORT=4031 bun index.html\" \"bun run api/server/index.ts\"",
    "build": "bun build api/index.ts --outdir dist --target bun && bun build.web.ts",
    "build:web": "bun build.web.ts",
    "start": "bun run api/server/index.ts",
    "test": "bun test tests/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.954.0",
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/eden": "^1.4.5",
    "@jejunetwork/config": "workspace:*",
    "@jejunetwork/contracts": "workspace:*",
    "@jejunetwork/db": "workspace:*",
    "@jejunetwork/kms": "workspace:*",
    "@jejunetwork/auth": "workspace:*",
    "@jejunetwork/sdk": "workspace:*",
    "@jejunetwork/shared": "workspace:*",
    "@jejunetwork/training": "workspace:*",
    "@jejunetwork/types": "workspace:*",
    "@noble/curves": "^1.9.7",
    "@noble/hashes": "^2.0.1",
    "@rainbow-me/rainbowkit": "2.2.9",
    "@solana/buffer-layout": "^4.0.1",
    "@solana/web3.js": "^1.98.0",
    "@tanstack/react-query": "^5.79.0",
    "croner": "^9.0.0",
    "elysia": "^1.4.19",
    "lru-cache": "11.2.4",
    "lucide-react": "^0.513.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.1",
    "tweetnacl": "^1.0.3",
    "viem": "2.43.3",
    "wagmi": "^2.17.4",
    "yaml": "^2.8.2",
    "zod": "4.2.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "typescript": "5.9.3"
  }
}
