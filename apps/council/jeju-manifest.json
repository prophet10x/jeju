{
  "name": "council",
  "version": "1.0.0",
  "description": "AI Council DAO - Fully autonomous governance with AI CEO and council agents",
  "type": "service",
  "maintainer": "jeju-network",
  "repository": "https://github.com/jejunetwork/jeju",
  "endpoints": {
    "a2a": "/a2a",
    "mcp": "/mcp",
    "rest": "/api/v1",
    "health": "/health"
  },
  "capabilities": {
    "governance": true,
    "proposals": true,
    "voting": true,
    "futarchy": true,
    "aiAgents": true,
    "reputation": true,
    "deepResearch": true
  },
  "integrations": {
    "compute": "@jeju/compute",
    "storage": "@jeju/storage",
    "leaderboard": "@jeju/leaderboard",
    "contracts": "@jeju/contracts"
  },
  "agents": [
    {
      "id": "eliza-ceo",
      "name": "Eliza",
      "role": "CEO",
      "description": "AI CEO - Final decision maker for approved proposals",
      "model": "claude-opus-4-5-20250514"
    },
    {
      "id": "council-treasury",
      "name": "Treasury Guardian",
      "role": "Council",
      "description": "Reviews financial impact and treasury implications"
    },
    {
      "id": "council-code",
      "name": "Code Guardian",
      "role": "Council",
      "description": "Reviews technical proposals and code changes"
    },
    {
      "id": "council-community",
      "name": "Community Guardian",
      "role": "Council",
      "description": "Reviews community impact and social implications"
    },
    {
      "id": "council-security",
      "name": "Security Guardian",
      "role": "Council",
      "description": "Reviews security implications and risk assessment"
    },
    {
      "id": "proposal-agent",
      "name": "Proposal Assistant",
      "role": "Intake",
      "description": "Helps users craft high-quality proposals"
    },
    {
      "id": "research-agent",
      "name": "Deep Researcher",
      "role": "Research",
      "description": "Conducts deep research on approved proposals using Opus 4.5"
    }
  ],
  "contracts": {
    "Council": "0x0000000000000000000000000000000000000000",
    "ProposalRegistry": "0x0000000000000000000000000000000000000000",
    "CEOAgent": "0x0000000000000000000000000000000000000000"
  },
  "testing": {
    "unit": {
      "command": "bun test tests/integration/compute-marketplace.test.ts",
      "timeout": 60000
    },
    "e2e": {
      "command": "bunx playwright test tests/synpress",
      "timeout": 300000,
      "requiresChain": true
    }
  }
}
