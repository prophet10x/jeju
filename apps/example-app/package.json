{
  "name": "@jejunetwork/example-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run src/server/index.ts",
    "dev:frontend": "bun run --hot src/frontend/dev.ts",
    "build": "bun run build.ts",
    "start": "bun run dist/server/index.js",
    "test": "bun test",
    "test:integration": "bun test src/tests/services.test.ts",
    "test:e2e": "bun run src/tests/e2e.test.ts",
    "test:synpress": "synpress run --config synpress.config.ts",
    "deploy": "bun run deploy.ts",
    "migrate": "bun run src/db/migrate.ts",
    "seed": "bun run scripts/seed.ts"
  },
  "dependencies": {
    "@jejunetwork/config": "workspace:*",
    "@jejunetwork/db": "workspace:*",
    "@jejunetwork/kms": "workspace:*",
    "@jejunetwork/oauth3": "workspace:*",
    "@jejunetwork/sdk": "workspace:*",
    "hono": "4.11.1",
    "viem": "2.43.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@synthetixio/synpress": "4.1.1",
    "@playwright/test": "1.57.0",
    "@types/bun": "latest",
    "typescript": "5.9.3"
  }
}
