{
  "name": "@jeju/cdn",
  "version": "1.0.0",
  "description": "Decentralized CDN - Edge caching, JNS gateway, and content delivery",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node",
    "start": "bun run dist/index.js",
    "test": "bun test",
    "test:e2e": "bun test tests/",
    "lint": "eslint src/",
    "edge-node": "bun run src/edge/index.ts",
    "coordinator": "bun run src/routing/coordinator.ts",
    "jns-gateway": "bun run src/gateway/jns-gateway.ts"
  },
  "dependencies": {
    "@jejunetwork/types": "workspace:*",
    "ethers": "^6.13.4",
    "hono": "^4.6.12",
    "lru-cache": "^11.0.2",
    "viem": "^2.21.45"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.6.3"
  }
}

