{
  "name": "@jejunetwork/compute",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Decentralized AI compute marketplace - ERC-8004 integrated with SSH/Docker rentals",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "bun run src/compute/node/index.ts",
    "dev:test": "PRIVATE_KEY=0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80 bun run src/compute/node/index.ts",
    "node": "bun run src/compute/node/index.ts",
    "mpc": "bun run src/mpc/mpc-server.ts",
    "mpc:dev": "MPC_NODE_ID=dev-node-1 MPC_VERBOSE=true bun run src/mpc/mpc-server.ts",
    "workers": "bun run src/compute/workers/server.ts",
    "workers:dev": "WORKER_SERVER_PORT=4020 bun run src/compute/workers/server.ts",
    "gateway": "bun run src/gateway/index.ts",
    "bridge": "bun run src/providers/server.ts",
    "bridge:dev": "ENABLE_AKASH=true AKASH_NETWORK=testnet bun run src/providers/server.ts",
    "bridge:testnet": "NETWORK=testnet AKASH_NETWORK=testnet bun run src/providers/server.ts",
    "bridge:mainnet": "NETWORK=mainnet AKASH_NETWORK=mainnet bun run src/providers/server.ts",
    "serve-frontend": "bun --port 4007 frontend/index.html",
    "demo": "bun run src/compute/scripts/demo.ts",
    "join": "bun run src/compute/scripts/join-network.ts",
    "deploy": "NETWORK=anvil bun run src/compute/scripts/deploy-base.ts",
    "deploy:sepolia": "NETWORK=sepolia bun run src/compute/scripts/deploy-base.ts",
    "deploy:mainnet": "NETWORK=mainnet bun run src/compute/scripts/deploy-base.ts",
    "validate": "bun run src/compute/scripts/validate.ts",
    "validate:contracts": "bun run src/compute/scripts/validate-deployment.ts",
    "validate:phala": "bun run src/compute/scripts/validate.ts --phala",
    "test": "bun test src/tests/ && bun test src/compute/tests/",
    "test:workers": "bun test src/compute/tests/workers.test.ts",
    "test:watch": "bun test --watch src/tests/",
    "test:e2e": "bun test src/compute/tests/e2e.test.ts",
    "test:bridge": "bun test src/providers/tests/akash.test.ts",
    "test:integration": "bun test src/providers/tests/integration.test.ts",
    "test:all": "bun test src/ --timeout 60000",
    "test:ui": "npx playwright test -c playwright.config.ts",
    "test:ui:headed": "npx playwright test -c playwright.config.ts --headed",
    "test:wallet": "npx playwright test -c synpress.config.ts",
    "test:wallet:headed": "npx playwright test -c synpress.config.ts --headed",
    "test:wallet:ui": "npx playwright test -c synpress.config.ts --ui",
    "test:wallet:cache": "npx synpress --force",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist out cache demo-output test-results",
    "docker:bridge": "docker build -f Dockerfile.bridge -t jeju-compute-bridge ."
  },
  "dependencies": {
    "@jeju/kms": "workspace:*",
    "@jejunetwork/types": "workspace:*",
    "arweave": "^1.15.7",
    "ethers": "^6.13.0",
    "gpt-tokenizer": "^3.4.0",
    "hono": "^4.10.7",
    "viem": "^2.21.0"
  },
  "devDependencies": {
    "@irys/sdk": "^0.2.11",
    "@jejunetwork/tests": "workspace:*",
    "@playwright/test": "^1.40.0",
    "@synthetixio/synpress": "^4.0.0",
    "@types/bun": "^1.3.3",
    "bs58": "^6.0.0",
    "typescript": "^5.6.3"
  }
}
