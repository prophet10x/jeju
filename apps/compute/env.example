# Jeju Compute - Environment Variables
# =====================================
# Copy to .env and fill in your values.
# Network config is loaded from @jejunetwork/config.

# === REQUIRED: Provider Wallet ===
PRIVATE_KEY=

# === Network Configuration ===
# JEJU_NETWORK=localnet  # localnet | testnet | mainnet
# RPC_URL=http://localhost:9545
# CHAIN_ID=1337

# === Contract Addresses (auto-loaded per network) ===
# COMPUTE_REGISTRY_ADDRESS=
# WORKER_REGISTRY_ADDRESS=
# LEDGER_ADDRESS=
# INFERENCE_ADDRESS=

# === Compute Node ===
# COMPUTE_PORT=4007
# MODEL_NAME=llama2
# MODEL_BACKEND=ollama  # ollama | mock | llamacpp
# MODEL_ENDPOINT=http://localhost:11434

# === Worker Runtime ===
# WORKER_SERVER_PORT=4020
# WORKER_DEFAULT_TIMEOUT_MS=30000
# WORKER_DEFAULT_MEMORY_MB=128

# === MPC Node ===
# MPC_PORT=4010
# MPC_NODE_ID=  # Auto-generated if not set
# MPC_THRESHOLD=2
# MPC_TOTAL_SHARES=3
# MPC_NETWORK_ID=jeju-localnet
# MPC_PEERS=  # nodeId@endpoint,nodeId@endpoint

# === Pricing ===
# PRICE_PER_INPUT_TOKEN=1000000000   # 1 gwei
# PRICE_PER_OUTPUT_TOKEN=2000000000  # 2 gwei

# === TEE Configuration ===
# TEE_ENDPOINT=  # Self-hosted TEE endpoint for remote mode
# TEE_SIMULATOR_ENDPOINT=  # For local development without hardware TEE

# === Provider Seeding (for operators) ===
# PROVIDER_PRIVATE_KEY=
# TESTNET_COMPUTE_REGISTRY_ADDRESS=
# TESTNET_WORKER_REGISTRY_ADDRESS=
# MAINNET_COMPUTE_REGISTRY_ADDRESS=
# MAINNET_WORKER_REGISTRY_ADDRESS=
