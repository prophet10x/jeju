{
  "name": "@jejunetwork/node",
  "version": "0.1.0",
  "description": "Network Node - Run infrastructure and earn rewards",
  "type": "module",
  "bin": {
    "jeju-node": "./src/cli.ts"
  },
  "scripts": {
    "dev": "vite",
    "dev:tauri": "tauri dev",
    "build": "vite build",
    "build:tauri": "tauri build",
    "preview": "vite preview",
    "tauri": "tauri",
    "app": "bun run src/app/index.ts",
    "app:start": "bun run src/app/index.ts start",
    "app:status": "bun run src/app/index.ts status",
    "app:setup": "bun run src/app/index.ts setup",
    "test": "bun test src tests/integration",
    "test:unit": "bun test src",
    "test:e2e": "playwright test tests/e2e",
    "test:synpress": "playwright test --config tests/synpress/synpress.config.ts",
    "test:integration": "bun test tests/integration",
    "test:all": "bun test src tests/integration && playwright test tests/e2e",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@jejunetwork/config": "workspace:*",
    "@jejunetwork/contracts": "workspace:*",
    "@jejunetwork/shared": "workspace:*",
    "@jejunetwork/sdk": "workspace:*",
    "@jejunetwork/types": "workspace:*",
    "@solana/web3.js": "1.98.4",
    "@tauri-apps/plugin-fs": "^2.4.4",
    "chalk": "5.4.1",
    "clsx": "2.1.1",
    "commander": "12.1.0",
    "date-fns": "4.1.0",
    "framer-motion": "12.23.26",
    "lru-cache": "^11.2.4",
    "lucide-react": "0.562.0",
    "prom-client": "^15.1.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "viem": "2.43.2",
    "webtorrent": "2.8.5",
    "ws": "^8.18.3",
    "zod": "^4.1.13",
    "zustand": "5.0.9"
  },
  "devDependencies": {
    "@jejunetwork/tests": "workspace:*",
    "@playwright/test": "1.57.0",
    "@synthetixio/synpress": "4.1.1",
    "@tauri-apps/api": "2.0.0",
    "@tauri-apps/cli": "2.0.0",
    "@tauri-apps/plugin-os": "2.0.0",
    "@tauri-apps/plugin-process": "2.0.0",
    "@tauri-apps/plugin-shell": "2.0.0",
    "@tauri-apps/plugin-store": "2.0.0",
    "@types/bun": "latest",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/webtorrent": "0.109.5",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "4.3.4",
    "autoprefixer": "10.4.21",
    "postcss": "8.5.3",
    "tailwindcss": "3.4.17",
    "typescript": "5.9.3",
    "vite": "6.3.5"
  }
}
