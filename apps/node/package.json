{
  "name": "@jejunetwork/node",
  "version": "0.1.0",
  "description": "Jeju Node - Run infrastructure and earn rewards",
  "type": "module",
  "bin": {
    "jeju-node": "./src/cli.ts"
  },
  "scripts": {
    "dev": "vite",
    "dev:tauri": "tauri dev",
    "build": "vite build",
    "build:tauri": "tauri build",
    "preview": "vite preview",
    "tauri": "tauri",
    "app": "bun run src/app/index.ts",
    "app:start": "bun run src/app/index.ts start",
    "app:status": "bun run src/app/index.ts status",
    "app:setup": "bun run src/app/index.ts setup",
    "test": "bun test src tests/integration",
    "test:unit": "bun test src",
    "test:e2e": "playwright test tests/e2e",
    "test:synpress": "synpress run --configFile tests/synpress/synpress.config.ts",
    "test:integration": "bun test tests/integration",
    "test:all": "bun test src tests/integration && playwright test tests/e2e",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@jejunetwork/config": "workspace:*",
    "@jejunetwork/contracts": "workspace:*",
    "@jejunetwork/types": "workspace:*",
    "ethers": "^6.11.1",
    "viem": "^2.7.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.5.0",
    "lucide-react": "^0.312.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.1",
    "framer-motion": "^11.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@tauri-apps/api": "^2.0.0",
    "@tauri-apps/cli": "^2.0.0",
    "@tauri-apps/plugin-os": "^2.0.0",
    "@tauri-apps/plugin-shell": "^2.0.0",
    "@tauri-apps/plugin-store": "^2.0.0",
    "@tauri-apps/plugin-process": "^2.0.0",
    "@types/bun": "latest",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "@playwright/test": "^1.56.1",
    "@synthetixio/synpress": "^4.0.0-alpha.7",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  }
}
