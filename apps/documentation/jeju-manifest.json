{
  "$schema": "../../packages/config/jeju-manifest.schema.json",
  "name": "documentation",
  "displayName": "Documentation",
  "version": "1.0.0",
  "type": "core",
  "description": "VitePress documentation site",
  "commands": {
    "dev": "bun run dev",
    "build": "bun run build",
    "start": "bun run preview"
  },
  "ports": {
    "main": 4004
  },
  "dependencies": [],
  "optional": true,
  "enabled": true,
  "autoStart": true,
  "tags": ["docs"],
  "agent": {
    "enabled": true,
    "tags": ["jeju-app", "documentation"],
    "metadata": {
      "category": "documentation",
      "provider": "jeju-network"
    },
    "trustModels": ["open"],
    "jnsName": "docs.jeju"
  },
  "testing": {
    "unit": {
      "command": "bun test tests/unit/",
      "timeout": 60000
    },
    "e2e": {
      "command": "bunx playwright test tests/e2e/",
      "timeout": 300000
    }
  }
}

