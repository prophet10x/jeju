{
  "name": "@jejunetwork/gateway",
  "version": "1.0.0",
  "type": "module",
  "description": "Gateway Portal - Protocol Infrastructure Hub. Bridge tokens, deploy paymasters, provide ETH liquidity, run nodes, earn rewards.",
  "scripts": {
    "dev": "concurrently \"PORT=4001 bun index.html\" \"bun src/a2a-server.ts\" \"bun src/rpc-server.ts\" \"bun src/x402-server.ts\" \"bun src/leaderboard-server.ts\"",
    "dev:jeju": "cd ../.. && bun run jeju:dev --only=gateway",
    "dev:ui": "PORT=4001 bun index.html",
    "dev:a2a": "bun src/a2a-server.ts",
    "dev:rpc": "bun src/rpc-server.ts",
    "dev:x402": "bun src/x402-server.ts",
    "dev:leaderboard": "bun src/leaderboard-server.ts",
    "leaderboard:ingest": "bun src/leaderboard/cli.ts ingest",
    "leaderboard:process": "bun src/leaderboard/cli.ts process",
    "leaderboard:export": "bun src/leaderboard/cli.ts export",
    "leaderboard:summarize": "bun src/leaderboard/cli.ts summarize",
    "leaderboard:all": "bun src/leaderboard/cli.ts all",
    "dev:solver": "bun src/solver/index.ts",
    "dev:oracle": "bun src/oracle-server.ts --network=localnet",
    "oracle": "bun src/oracle-server.ts",
    "oracle:testnet": "bun src/oracle-server.ts --network=testnet",
    "oracle:mainnet": "bun src/oracle-server.ts --network=mainnet",
    "oracle:validate": "bun src/oracle-server.ts --dry-run",
    "build": "bun build index.html --outdir=dist --minify",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "test": "bun run test:unit && bun run test:rpc && bun run test:x402 && bun run test:oracle && bun run test:leaderboard",
    "test:full": "bun run test:unit && bun run test:rpc && bun run test:a2a && bun run test:x402 && bun run test:oracle && bun run test:leaderboard",
    "test:leaderboard": "bun test tests/leaderboard/",
    "test:x402": "bun test tests/x402/",
    "test:unit": "bun test src/lib/tests/",
    "test:rpc": "bun test tests/rpc/",
    "test:contracts": "bun test tests/contracts/",
    "test:a2a": "bun test tests/a2a/",
    "test:oracle": "bun test tests/oracle/price-fetcher.test.ts tests/oracle/node.test.ts",
    "test:oracle:unit": "bun test tests/oracle/price-fetcher.test.ts tests/oracle/node.test.ts",
    "test:oracle:integration": "INTEGRATION_TESTS=1 bun test tests/oracle/integration.test.ts",
    "test:e2e": "playwright test tests/synpress",
    "test:e2e:smoke": "playwright test tests/synpress/00-basic.spec.ts --headed",
    "test:e2e:flows": "playwright test tests/synpress/flows/ --headed",
    "test:e2e:transactions": "playwright test tests/synpress/transactions/ --headed",
    "test:e2e:pages": "playwright test tests/synpress/pages/ --headed",
    "test:e2e:visual": "playwright test tests/synpress/visual/ --headed",
    "test:e2e:coverage": "playwright test tests/synpress/coverage/ --headed",
    "test:e2e:bridge": "playwright test tests/synpress/bridge/ --headed",
    "test:e2e:navigation": "playwright test tests/synpress/navigation/ --headed",
    "test:e2e:components": "playwright test tests/synpress/components/ --headed",
    "test:e2e:integration": "playwright test tests/synpress/integration/ --headed",
    "test:e2e:edge-cases": "playwright test tests/synpress/edge-cases/ --headed",
    "test:e2e:multi-token": "playwright test tests/synpress/multi-token/ --headed",
    "test:e2e:responsive": "playwright test tests/e2e/responsive-buttons.spec.ts --headed",
    "test:e2e:responsive:ci": "playwright test tests/e2e/responsive-buttons.spec.ts",
    "test:e2e:headed": "playwright test tests/synpress --headed",
    "test:e2e:debug": "playwright test tests/synpress --debug",
    "test:e2e:old": "playwright test tests/e2e",
    "test:integration": "playwright test tests/integration/",
    "test:watch": "bun test --watch",
    "test:all": "bun run test && bun run test:e2e",
    "test:jeju": "cd ../.. && bun run jeju:test --app=gateway",
    "test:report": "playwright show-report"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.0",
    "@jejunetwork/config": "workspace:*",
    "@jejunetwork/db": "workspace:*",
    "@jejunetwork/dws": "workspace:*",
    "@jejunetwork/kms": "workspace:*",
    "@jejunetwork/oauth3": "workspace:*",
    "@jejunetwork/shared": "workspace:*",
    "@jejunetwork/types": "workspace:*",
    "@jejunetwork/ui": "workspace:*",
    "@rainbow-me/rainbowkit": "2.2.9",
    "@tanstack/react-query": "5.90.12",
    "elysia": "^1.4.19",
    "hono": "4.11.1",
    "lru-cache": "^11.0.0",
    "lucide-react": "0.562.0",
    "opossum": "^9.0.0",
    "p-retry": "^7.1.1",
    "prom-client": "^15.1.3",
    "rate-limiter-flexible": "^6.0.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-router-dom": "6.28.0",
    "recharts": "3.6.0",
    "viem": "2.43.2",
    "wagmi": "2.18.1",
    "ws": "8.18.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@jejunetwork/tests": "workspace:*",
    "@playwright/test": "1.57.0",
    "@synthetixio/synpress": "4.1.1",
    "@types/bun": "latest",
    "@types/node": "22.15.30",
    "@types/ws": "8.18.1",
    "@types/react": "19.2.3",
    "@types/react-dom": "19.2.3",
    "concurrently": "9.1.0",
    "playwright-core": "1.57.0",
    "typescript": "5.9.3"
  }
}
