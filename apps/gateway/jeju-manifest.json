{
  "$schema": "../../packages/config/jeju-manifest.schema.json",
  "name": "gateway",
  "displayName": "Gateway Portal",
  "version": "1.0.0",
  "type": "core",
  "description": "Protocol Infrastructure Hub: Bridge tokens, deploy paymasters, provide ETH liquidity, run nodes, earn rewards, cross-chain intents. Supports elizaOS, CLANKER, VIRTUAL, CLANKERMON + any ERC20. Protocol power = paymaster + liquidity.",
  "commands": {
    "dev": "bun run dev",
    "build": "bun run build",
    "start": "bun run preview",
    "solver": "bun src/solver/index.ts"
  },
  "ports": {
    "main": 4001,
    "nodeApi": 4002,
    "a2a": 4003,
    "ws": 4012
  },
  "dependencies": ["contracts", "config"],
  "optional": false,
  "enabled": true,
  "autoStart": true,
  "tags": ["protocol", "infrastructure", "paymaster", "nodes", "staking", "liquidity", "elizaOS", "multi-token", "intents", "cross-chain", "oif", "aggregator", "solver"],
  "jns": {
    "name": "gateway.jeju",
    "description": "Protocol Infrastructure Hub for the Network",
    "url": "https://gateway.jeju.network"
  },
  "agent": {
    "enabled": true,
    "a2aEndpoint": "http://localhost:4003/a2a",
    "mcpEndpoint": "http://localhost:4003/mcp",
    "tags": ["jeju-app", "protocol", "infrastructure", "nodes", "staking", "intents", "cross-chain", "oif"],
    "metadata": {
      "category": "infrastructure",
      "provider": "jeju-network"
    },
    "trustModels": ["open"],
    "x402Support": true,
    "jnsName": "gateway.jeju"
  },
  "healthCheck": {
    "url": "http://localhost:4003/health",
    "interval": 30000
  },
  "testing": {
    "unit": {
      "command": "bun run test:unit",
      "timeout": 120000
    },
    "integration": {
      "command": "bun run test:rpc",
      "timeout": 120000,
      "requiresServices": true
    },
    "e2e": {
      "command": "bunx playwright test tests/synpress",
      "config": "synpress.config.ts",
      "timeout": 600000,
      "requiresChain": true,
      "requiresWallet": true
    },
    "services": ["contracts"]
  }
}
